{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"esModuleInterop": true,
		"isolatedModules": true,
		"lib": ["dom", "esnext"],
		"module": "ESNext",
		"moduleDetection": "force",
		"moduleResolution": "bundler",
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"skipLibCheck": true,
		"strict": true,
		"target": "ESNext"
	},
	"exclude": ["node_modules"],
	"include": ["**/*.ts"]
}
